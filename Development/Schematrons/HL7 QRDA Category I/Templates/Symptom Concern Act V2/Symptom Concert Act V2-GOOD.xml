<!-- 		
    Title:  Symptom Concern Act V2, Minimal sample
    Author: Dan Donahue
    Date:   03-02-2017
    
    Total Errors Expected: 0
    Total Warnings Expected: 0
-->

<ClinicalDocument xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:hl7-org:v3 ../Schema/CDA/infrastructure/cda/CDA_SDTC.xsd" xmlns="urn:hl7-org:v3"
	xmlns:voc="urn:hl7-org:v3/voc" xmlns:sdtc="urn:hl7-org:sdtc">
	<entry>
		<act classCode="ACT" moodCode="EVN">
			<!-- Symptom Concern Act (V2) -->
			<templateId root="2.16.840.1.113883.10.20.24.3.138" extension="2016-08-01"/>
			<!-- The statusCode represents whether this is an active (active) or resolved (completed) symptom -->
			<statusCode code="active"/>
			<!-- This is the time that the symptom is authored in the patient's chart -->
			<effectiveTime>
				<!-- equivalent to author/time in the contained observation -->
				<low value="201504081130"/>
			</effectiveTime>
			<entryRelationship typeCode="SUBJ">
				<observation classCode="OBS" moodCode="EVN">
					<!-- Symptom template -->
					<templateId root="2.16.840.1.113883.10.20.24.3.136"/>
				</observation>
			</entryRelationship>
		</act>
	</entry>
</ClinicalDocument>
